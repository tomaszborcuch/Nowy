<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tomek.londonschoolapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SlowkaListActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Kategorie"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".cialo"
            android:parentActivityName=".Kategorie"/>
        <activity android:name=".cialo_adapter" />
        <activity android:name=".Quiz" />
        <activity android:name=".podroze_activity"
            android:parentActivityName=".Kategorie"/>
        <activity android:name=".Podroze_adapter" />
        <activity android:name=".Jedzenie_activity"
            android:parentActivityName=".Kategorie"/>
        <activity android:name=".Jedzenie_adapter"></activity>
    </application>

</manifest>